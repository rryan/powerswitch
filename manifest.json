{
  "manifest_version": 2,
  "name": "PowerSwitch",
  "version": "2017.2.19",
  "description": "Switch tabs and open bookmarks by typing",
  "background": {
    "scripts": ["MochiKit.js", "background.js"]
  },
  "commands": {
    "switch_tab": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Bring up the tab switcher"
    },
    "switch_bookmark": {
      "suggested_key": {
        "default": "Ctrl+I"
      },
      "description": "Bring up the bookmark switcher"
    }
  },
  "permissions": [
    "tabs", "bookmarks"
  ]
}
